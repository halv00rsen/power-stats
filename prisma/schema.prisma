// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Measurement {
  id         String   @id @default(uuid())
  month String

  from String
  to String
  cost Float
  unitPrice Float
  unitPriceVAT Float
  consumption Float
  consumptionUnit String
}
